def is_power(a, b):
    if a == b:
        return True

    temp = b

    while (temp <= a):
        if temp == a:
            return True
        temp *= b

    return False


is_power(1024, 2)


is_power(6561, 3)


is_power(4374, 3)


is_power(768, 2)





def gcd(a, b):
    if b == 0:
        return a
    else:
        return gcd(b, a % b)


gcd(21, 7)


